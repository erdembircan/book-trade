{
  "presets": [
    "react",
    "env"
  ],
  "plugins": [
    [
      "dynamic-import-webpack"
    ],
    [
      "syntax-dynamic-import"
    ],
    [
      "transform-runtime",
      {
        "helpers": false,
        "polyfill": false,
        "regenerator": true,
        "moduleName": "babel-runtime"
      }
    ]
  ]
}