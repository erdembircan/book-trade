{
  "name": "book-trade",
  "version": "1.0.0",
  "description": "freeCodeCamp Book Trading Project",
  "main": "index.js",
  "scripts": {
    "client-analyse": "NODE_ENV=analyse webpack --config webpack.client.config.js -p",
    "build-server-dev": "webpack --config webpack.server.config.js --watch --progress --colors",
    "build-client-dev": "webpack --config webpack.client.config.js --watch --progress --colors",
    "build-dev":
      "concurrently -n \"server,client\" -c \"magenta,cyan\" \"npm run build-server-dev\" \"npm run build-client-dev\"",
    "start-dev": "NODE_ENV=development nodemon build/server/server.js",
    "test": "jest --verbose"
  },
  "jest": {
    "testRegex": ".*.test.js$"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/erdembircan/book-trade.git"
  },
  "author": "Erdem Bircan <erdem.erdembircan@gmail.com> (https://erdembircan.github.io/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/erdembircan/book-trade/issues"
  },
  "homepage": "https://github.com/erdembircan/book-trade#readme",
  "dependencies": {
    "body-parser": "^1.18.2",
    "chalk": "^2.3.0",
    "compression": "^1.7.1",
    "cookie-parser": "^1.4.3",
    "env-data": "^1.0.0",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "helmet": "^3.9.0",
    "minimist": "^1.2.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^3.5.1",
    "folder-delete": "^1.0.3",
    "jest": "^22.0.6",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.9.2",
    "webpack-node-externals": "^1.6.0"
  }
}
